{
  "Logging": {
    "LogLevel": {
      "Default": "Information", // Altera o padrão para Warning (ignora informações automáticas)
      "Microsoft": "Information", // Ignora informações do sistema
      "System": "Information", // Ignora informações do .NET Core
      "DiaDePromo": "Information" // Define Information para sua aplicação
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "DiaDePromo": "Information"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "MongoDbSettings": {
    "ConnectionString": "mongodb://admin:admin_1234@localhost:27017/"
  }
}
